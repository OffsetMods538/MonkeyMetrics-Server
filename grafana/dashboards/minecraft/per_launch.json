      "id": 1,
      "type": "timeseries",
      "title": "Total Minecraft version usage",
      "description": "Total amount of game launches recorded per Minecraft version",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-main"
      },
      "gridPos": {
        "w": 12,
        "h": 8,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by(minecraft_version) (minecraft_launches_total{minecraft_version=~\"$minecraft_version\", mod_loader=~\"$mod_loader\", environment=~\"$environment\"})",
          "legendFormat": "__auto"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
